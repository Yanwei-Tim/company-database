    <sql>SELECT A.DEVICE_ID, NVL&#x28;B.UPDATE_STATUS, A.STATUS&#x29; STATUS--&#x5df2;&#x6709;&#x8bbe;&#x5907;&#x72b6;&#x6001;&#x53d8;&#x66f4;&#x8868;&#xd;&#xa;  FROM &#x28;SELECT T.DEVICE_ID, T.STATUS&#xd;&#xa;          FROM X_D_D_DEVICE_STATUS T&#xd;&#xa;         WHERE T.BEGIN_TIME &#x3d; to_date&#x28;&#x27;&#x24;&#x7b;timeStamp&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29; - 1&#x29; A,&#xd;&#xa;       &#x28;SELECT A.DEVICE_ID,A.UPDATE_TIME,A.UPDATE_STATUS&#xd;&#xa;  FROM &#x28;SELECT ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY DEVICE_ID, TRUNC&#x28;UPDATE_TIME&#x29; ORDER BY UPDATE_TIME DESC&#x29; ROW_NUM,&#xd;&#xa;               A.DEVICE_ID,&#xd;&#xa;               TRUNC&#x28;A.UPDATE_TIME&#x29; UPDATE_TIME,&#xd;&#xa;               A.UPDATE_STATUS&#xd;&#xa;          FROM &#x28;SELECT T.DEVICE_ID, T.UPDATE_TIME, T.UPDATE_STATUS&#xd;&#xa;                  FROM T_DEVICE_SYS_USE_STATUS T&#xd;&#xa;                 WHERE TRUNC&#x28;T.UPDATE_TIME&#x29; &#x3d; to_date&#x28;&#x27;&#x24;&#x7b;timeStamp&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                   AND T.DEVICE_ID IS NOT NULL&#x29; A&#x29; A&#xd;&#xa; where ROW_NUM &#x3d; 1&#x29;b where a.device_id &#x3d; b.device_id&#x28;&#x2b;&#x29;&#xd;&#xa;&#xd;&#xa;union--&#x5408;&#x5e76;&#x8fde;&#x63a5;&#xd;&#xa;&#xd;&#xa;SELECT A.DEVICE_ID, NVL&#x28;B.UPDATE_STATUS, A.USE_STATUS_FLAG&#x29; STATUS--&#x65b0;&#x521b;&#x5efa;&#x8bbe;&#x5907;&#x53d8;&#x66f4;&#x8bb0;&#x5f55;&#x8868;&#xd;&#xa;  FROM &#x28;SELECT T.DEVICE_ID, &#x27;0&#x27; as USE_STATUS_FLAG --&#x521b;&#x5efa;&#x65f6;&#x8ba4;&#x5b9a;&#x4e3a;&#x672a;&#x542f;&#x7528; &#xd;&#xa;          FROM T_DEVICE_SYS T&#xd;&#xa;         WHERE TRUNC&#x28;T.CREATE_TIME&#x29; &#x3d; TO_DATE&#x28;&#x27;&#x24;&#x7b;timeStamp&#x7d;&#x27;, &#x27;yyyy-MM-dd&#x27;&#x29;&#x29;A,&#xd;&#xa;        &#x28;SELECT A.DEVICE_ID, A.UPDATE_TIME, A.UPDATE_STATUS&#xd;&#xa;          FROM &#x28;SELECT ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY DEVICE_ID, TRUNC&#x28;UPDATE_TIME&#x29; ORDER BY UPDATE_TIME DESC&#x29; ROW_NUM,&#xd;&#xa;                       A.DEVICE_ID,&#xd;&#xa;                       TRUNC&#x28;A.UPDATE_TIME&#x29; UPDATE_TIME,&#xd;&#xa;                       A.UPDATE_STATUS&#xd;&#xa;                  FROM &#x28;SELECT T.DEVICE_ID, T.UPDATE_TIME, T.UPDATE_STATUS&#xd;&#xa;                          FROM T_DEVICE_SYS_USE_STATUS T&#xd;&#xa;                         WHERE TRUNC&#x28;T.UPDATE_TIME&#x29; &#x3d; to_date&#x28;&#x27;&#x24;&#x7b;timeStamp&#x7d;&#x27;, &#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;                           AND T.DEVICE_ID IS NOT NULL&#x29; A&#x29; A&#xd;&#xa;         WHERE A.ROW_NUM &#x3d; 1&#x29; B&#xd;&#xa;         WHERE A.DEVICE_ID &#x3d; B.DEVICE_ID&#x28;&#x2b;&#x29;&#xd;&#xa;</sql>
