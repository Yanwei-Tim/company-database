  <step>
    <name>monthOnlineRate</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>ORCL</connection>
    <sql>SELECT T.DEVICE_ID,&#xd;&#xa;       T.ORG_ID,&#xd;&#xa;       T.DEVICE_TYPE,&#xd;&#xa;       T.CONTRACT_ID,&#xd;&#xa;       T.CONTRACTOR_ID,&#xd;&#xa;       STATISTICS_MONTH,&#xd;&#xa;       ROUND&#x28;AVG&#x28;T.PRIMARY_ONLINE&#x29;,4&#x29; PRIMARY_ONLINE,&#xd;&#xa;       ROUND&#x28;AVG&#x28;CORRECT_ONLINE&#x29;,4&#x29; CORRECT_ONLINE,&#xd;&#xa;       T.OWNERSHIP&#xd;&#xa;  FROM&#x28;SELECT T.DEVICE_ID,&#xd;&#xa;              T.ORG_ID,&#xd;&#xa;              T.DEVICE_TYPE,&#xd;&#xa;              T.CONTRACT_ID,&#xd;&#xa;              T.CONTRACTOR_ID,&#xd;&#xa;              TRUNC&#x28;T.STATISTICS_DATE, &#x27;MM&#x27;&#x29; STATISTICS_MONTH,&#xd;&#xa;              CASE WHEN T.DURATION_SECS IS NULL THEN 0 WHEN T.DURATION_SECS&#x3e;&#x3d;86400 THEN 1 ELSE T.DURATION_SECS&#x2f;86400 END AS PRIMARY_ONLINE, --&#x5982;&#x679c;&#x5e94;&#x5728;&#x7ebf;&#x4e3a;0&#xff0c;&#x5728;&#x7ebf;&#x65f6;&#x957f;&#x4e3a;null,&#x4e0d;&#x53c2;&#x4e0e;&#x8ba1;&#x7b97;&#xff0c;&#x5982;&#x679c;&#x5e94;&#x5728;&#x7ebf;&#x65f6;&#x957f;&#x4e0d;&#x4e3a;null,&#x8ba1;&#x7b97;&#x5728;&#x7ebf;&#xd;&#xa;              CASE WHEN T.CORRECT_SHOULD_SECS IS NULL THEN 0 WHEN &#x28;T.DURATION_SECS-T.FAULT_SECS&#x29;&#x3e;&#x3d;T.CORRECT_SHOULD_SECS THEN 1 ELSE &#x28;T.DURATION_SECS-T.FAULT_SECS&#x29;&#x2f;T.CORRECT_SHOULD_SECS END AS CORRECT_ONLINE,&#xd;&#xa;              T.OWNERSHIP&#xd;&#xa;        FROM X_D_D_STATUS_LOG T&#xd;&#xa;        WHERE TRUNC&#x28;T.STATISTICS_DATE,&#x27;MM&#x27;&#x29; &#x3d; TRUNC&#x28;&#x3f;,&#x27;MM&#x27;&#x29;&#xd;&#xa;       &#x29;T GROUP BY T.DEVICE_ID,&#xd;&#xa;       T.ORG_ID,&#xd;&#xa;       T.DEVICE_TYPE,&#xd;&#xa;       T.CONTRACT_ID,&#xd;&#xa;       T.CONTRACTOR_ID,&#xd;&#xa;       STATISTICS_MONTH,&#xd;&#xa;       T.OWNERSHIP</sql>
