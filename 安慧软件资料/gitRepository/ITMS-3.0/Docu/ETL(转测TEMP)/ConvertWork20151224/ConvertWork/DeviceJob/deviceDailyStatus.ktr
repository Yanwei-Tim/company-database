  <step>
    <name>getOnlineRate</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>ORCL</connection>
    <sql>SELECT T.DEVICE_ID,&#xd;&#xa;       V.ORG_ID,&#xd;&#xa;       V.DEVICE_TYPE,&#xd;&#xa;       T.CONTRACT_ID,&#xd;&#xa;       T.CONTRACTOR_ID,&#xd;&#xa;       T.BEGIN_TIME,&#xd;&#xa;       T.BEGIN_TIME    AS END_TIME,&#xd;&#xa;       T.UPDATE_STATUS AS STATUS,&#xd;&#xa;       T.STAT_ONLINE&#xd;&#xa;  FROM &#x28;SELECT NVL&#x28;B.DEVICE_ID, T.DEVICE_ID&#x29; DEVICE_ID,&#xd;&#xa;               NVL&#x28;B.CONTRACT_ID,T.CONTRACT_ID&#x29; CONTRACT_ID,&#xd;&#xa;               NVL&#x28;B.CONTRACTOR_ID,T.CONTRACTOR_ID&#x29; CONTRACTOR_ID,&#xd;&#xa;               NVL&#x28;B.UPDATE_TIME, &#x3f;&#x29; BEGIN_TIME,&#xd;&#xa;               NVL&#x28;B.UPDATE_STATUS, T.STATUS&#x29; UPDATE_STATUS,&#xd;&#xa;               NVL&#x28;B.STAT_ONLINE, T.STAT_ONLINE&#x29; STAT_ONLINE&#xd;&#xa;          FROM &#x28;SELECT T.DEVICE_ID,&#xd;&#xa;                       V.CONTRACT_ID,&#xd;&#xa;                       V.CONTRACTOR_ID,&#xd;&#xa;                       &#x3f; AS BEGIN_TIME,&#xd;&#xa;                       T.STATUS,&#xd;&#xa;                       T.STAT_ONLINE&#xd;&#xa;                  FROM X_D_D_DEVICE_STATUS T, T_SYS_DEVICE_VIEW V&#xd;&#xa;                 WHERE T.BEGIN_TIME &#x3d; &#x3f; - 1&#xd;&#xa;                   AND T.DEVICE_ID &#x3d; V.DEVICE_ID&#x29; T&#xd;&#xa;          FULL JOIN &#x28;SELECT A.&#x2a;&#xd;&#xa;                      FROM &#x28;SELECT ROW_NUMBER&#x28;&#x29; OVER&#x28;PARTITION BY DEVICE_ID, TRUNC&#x28;UPDATE_TIME&#x29; ORDER BY UPDATE_TIME DESC&#x29; R,&#xd;&#xa;                                   A.DEVICE_ID,A.CONTRACT_ID,A.CONTRACTOR_ID,&#xd;&#xa;                                   TRUNC&#x28;A.UPDATE_TIME&#x29; UPDATE_TIME,&#xd;&#xa;                                   A.UPDATE_STATUS,&#xd;&#xa;                                   A.STAT_ONLINE&#xd;&#xa;                              FROM &#x28;SELECT T.DEVICE_ID,&#xd;&#xa;                                           V.CONTRACT_ID,&#xd;&#xa;                                           V.CONTRACTOR_ID,&#xd;&#xa;                                           T.UPDATE_TIME,&#xd;&#xa;                                           T.UPDATE_STATUS,&#xd;&#xa;                                           V.STAT_ONLINE&#xd;&#xa;                                      FROM T_DEVICE_SYS_USE_STATUS T&#xd;&#xa;                                      LEFT JOIN &#x28;SELECT T.DEVICE_ID,T.CONTRACT_ID,T.CONTRACTOR_ID,&#xd;&#xa;                                                      CASE WHEN  FUNC_FUNTION_FILTER&#x28;T.FUNCTION_CODE_LISTS,&#x27;2004&#x27;&#x29; &#x3d;&#x27;true&#x27; THEN 1 ELSE 0 END AS STAT_ONLINE&#xd;&#xa;                                                  FROM T_SYS_DEVICE_VIEW T WHERE T.DEVICE_ID IS NOT NULL --test&#xd;&#xa;                                                  &#x29; V&#xd;&#xa;                                        ON T.DEVICE_ID &#x3d; V.DEVICE_ID&#xd;&#xa;                                     WHERE TRUNC&#x28;T.UPDATE_TIME&#x29; &#x3d; &#x3f; AND T.DEVICE_ID IS NOT NULL&#xd;&#xa;                                     ORDER BY T.DEVICE_ID, T.UPDATE_TIME&#x29; A&#x29; A&#xd;&#xa;                     WHERE A.R &#x3d; 1&#x29; B&#xd;&#xa;            ON T.DEVICE_ID &#x3d; B.DEVICE_ID&#x29; T,&#xd;&#xa;       T_SYS_DEVICE_VIEW V&#xd;&#xa;   WHERE T.DEVICE_ID &#x3d; V.DEVICE_ID</sql>
    <limit>0</limit>
    <lookup>setDateParam</lookup>
