    <connection>ORCL</connection>
    <sql>SELECT DEVICE_ID,&#xd;&#xa;           ORG_ID,&#xd;&#xa;           DEVICE_TYPE,&#xd;&#xa;           CONTRACT_ID,&#xd;&#xa;           CONTRACTOR_ID,&#xd;&#xa;           STATISTICS_DATE,&#xd;&#xa;           DECODE&#x28;FAULT_SECS, 0, DECODE&#x28;DURATION_SECS, NULL, &#x27;3&#x27;, &#x27;1&#x27;&#x29;, &#x27;2&#x27;&#x29; AS ONLINE_STATUS,-- 1,&#x6570;&#x636e;&#x6b63;&#x5e38; 2 &#x8be5;&#x65e5;&#x6709;&#x6545;&#x969c; 3 &#x8be5;&#x65e5;&#x672a;&#x6709;&#x6b63;&#x5e38;&#x6570;&#x636e;&#xd;&#xa;           DURATION_SECS,--&#x6b63;&#x5e38;&#x5728;&#x7ebf;&#x65f6;&#x957f;&#xd;&#xa;           FAULT_SECS,--&#x6545;&#x969c;&#x65f6;&#x957f;&#xd;&#xa;           FAULT_TIMES,--&#x6545;&#x969c;&#x6b21;&#x6570; ,&#xd;&#xa;           STAT_ONLINE,&#xd;&#xa;           24 &#x2a; 60 &#x2a; 60 - FAULT_SECS AS CORRECT_SHOULD_SECS--&#x5e94;&#x5728;&#x7ebf;&#x65f6;&#x957f;&#xd;&#xa;      FROM &#x28;SELECT T.DEVICE_ID,&#xd;&#xa;                   T.ORG_ID,&#xd;&#xa;                   T.DEVICE_TYPE,&#xd;&#xa;                   T.CONTRACT_ID,&#xd;&#xa;                   T.CONTRACTOR_ID,&#xd;&#xa;                   &#x3f; AS STATISTICS_DATE,&#xd;&#xa;                   V.DURATION_SECS,&#xd;&#xa;                   V.FAULT_SECS,&#xd;&#xa;                   V.FAULT_TIMES,&#xd;&#xa;                   T.STAT_ONLINE&#xd;&#xa;              FROM &#x28;SELECT T.DEVICE_ID,&#xd;&#xa;                           T.ORG_ID,&#xd;&#xa;                           T.DEVICE_TYPE,&#xd;&#xa;                           T.CONTRACT_ID,&#xd;&#xa;                           T.CONTRACTOR_ID,&#xd;&#xa;                           T.BEGIN_TIME,&#xd;&#xa;                           T.STATUS,&#xd;&#xa;                           T.STAT_ONLINE&#xd;&#xa;                      FROM X_D_D_DEVICE_STATUS T&#xd;&#xa;                     WHERE T.STATUS &#x3d; &#x27;1&#x27;&#xd;&#xa;                       --AND T.STAT_ONLINE &#x3d; &#x27;1&#x27;&#xd;&#xa;                       AND T.BEGIN_TIME &#x3d; &#x3f;&#x29; T&#xd;&#xa;              LEFT JOIN &#x28;SELECT T.DEVICE_ID,&#xd;&#xa;                              SUM&#x28;DECODE&#x28;T.STATUS_TYPE,&#x27;1&#x27;,T.DURATION_SECS,&#x27;3&#x27;,T.DURATION_SECS,&#x27;4&#x27;,T.DURATION_SECS,0&#x29;&#x29; DURATION_SECS,--1 &#x6b63;&#x5e38; 4 &#x5f02;&#x5e38; &#x4e0d;&#x5f71;&#x54cd;&#x4e1a;&#x52a1; &#x7b97;&#x4e3a;&#x5728;&#x7ebf;&#xd;&#xa;                              SUM&#x28;DECODE&#x28;T.STATUS_TYPE,&#x27;3&#x27;,T.DURATION_SECS,0&#x29;&#x29; FAULT_SECS, --3 &#x6545;&#x969c;  4&#x8131;&#x673a; &#x7b97;&#x5f71;&#x54cd;&#x4e1a;&#x52a1; &#x7b97;&#x4e0d;&#x5728;&#x7ebf; &#xd;&#xa;                              SUM&#x28;DECODE&#x28;T.STATUS_TYPE,&#x27;3&#x27;,1,0&#x29;&#x29; FAULT_TIMES&#xd;&#xa;                          FROM T_DEVICE_STATUS_LOG T&#xd;&#xa;                         WHERE TRUNC&#x28;T.START_TIME&#x29; &#x3d; &#x3f;&#xd;&#xa;                         GROUP BY T.DEVICE_ID&#x29; V&#xd;&#xa;                ON T.DEVICE_ID &#x3d; V.DEVICE_ID&#x29; T</sql>
    <limit>0</limit>
